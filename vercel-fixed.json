{
  "version": 2,
  "functions": {
    "api/vercel_handler.py": {
      "runtime": "python3"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/vercel_handler.py"
    },
    {
      "src": "/health",
      "dest": "/api/vercel_handler.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/vercel_handler.py"
    }
  ]
}